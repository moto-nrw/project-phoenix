meta {
  name: Dev Rooms
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/api/rooms
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

script:post-response {
  if (res.status === 200) {
    console.log(`✅ Rooms API OK - ${res.body.data?.length || 0} rooms`);
  } else {
    console.log("❌ Rooms API failed");
  }
}