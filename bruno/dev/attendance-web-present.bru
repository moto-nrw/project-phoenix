meta {
  name: Web Attendance Present
  type: http
  seq: 34
}

get {
  url: {{baseUrl}}/api/active/attendance/present
  body: none
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

script:post-response {
  if (res.status === 200 && res.body.data) {
    const students = res.body.data;
    console.log(`✅ Present students retrieved - ${students.length} students present`);
    
    students.forEach(s => {
      console.log(`   ✓ ${s.student_name} (${s.group_name || 'No group'}) - Checked in at ${new Date(s.check_in_time).toLocaleTimeString()}`);
    });
  } else {
    console.log("❌ Get present students failed:", res.body?.message || "Unknown error");
  }
}