meta {
  name: Dev Auth
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/auth/login
  body: json
  auth: none
}

body:json {
  {
    "email": "admin@example.com",
    "password": "Test1234%"
  }
}

script:post-response {
  if (res.status === 200) {
    bru.setEnvVar("accessToken", res.body.access_token);
    console.log("✅ Admin login OK");
  } else {
    console.log("❌ Login failed");
  }
}